{
  "apiVersion": "v1",
  "kind": "Secret",
  "metadata": {
    "name": "gerrit-http-password",
    "namespace": "gerrit",
    "labels": {
      "app": "gerrit"
    },
    "annotations": {
      "description": "Gerrit Admin HTTP Token"
    }
  },
  "type": "Opaque",
  "stringData": {
    "username": "gerrit-admin"
  },
  "data": {
    "password": "##HTTP_PASSWORD##"
  }
}